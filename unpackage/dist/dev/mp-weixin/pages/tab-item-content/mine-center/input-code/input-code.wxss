page {
  background: #fff;
}
.code-content {
  text-align: center;
  padding-top: 56px;
  padding-right: 32px;
  padding-left: 38px;
  font-size: 15px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #2a2a2a;
}
.code-content .input-content {
  margin-top: 38px;
  padding-left: 50px;
  padding-right: 50px;
  margin-bottom: 24px;
}
.code-content .box-item {
  width: 43px;
  height: 43px;
  border-radius: 4px;
  line-height: 43px;
  border: 1px solid #42b0ed;
  background: #f2f2f2;
}
.code-content .again {
  font-size: 14px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #42b0ed;
  margin-bottom: 24px;
}
.notice {
  font-size: 16px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #42b0ed;
}

