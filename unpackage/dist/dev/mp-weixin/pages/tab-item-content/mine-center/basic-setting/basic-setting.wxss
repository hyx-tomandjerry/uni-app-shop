  height: 74px;
  line-height: 74px;
  border-bottom: 1px solid #EEEEED;
}
.header-content .header-title {
  font-size: 16px;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: #2a2a2a;
}
.header-content .headers-submit {
  font-size: 14px;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: #42b0ed;
}
.info-content-item {
  padding: 1px  33px 2px 14px;
  line-height: 47px;
