page {
  background: #fff;
}
.login_container {
  padding: 47px 12px 17px 15px;
}
.login_container .title {
  padding-left: 7px;
}
.login_container .title .login_title {
  height: 42px;
  line-height: 42px;
  font-size: 30px;
  font-weight: 600;
  color: #2a2a2a;
}
.login_container .title .login_design {
  width: 195px;
  height: 21px;
  font-size: 15px;
  font-weight: 400;
  line-height: 21px;
}
.login_container .login_form {
  margin-top: 28px;
}
.login_container .forget {
  font-size: 14px;
  font-weight: 400;
  color: #2a2a2a;
  height: 20px;
  line-height: 20px;
}
.cu-form-group {
  height: 60px;
  line-height: 60px;
}
.cu-form-group uni-input {
  font-size: 16px;
  font-weight: 400;
  color: #b9b9b9;
}
.cu-form-group + .cu-form-group {
  border-bottom: 0.5px solid #eee;
}
.cu-btn {
  background: #ffffff;
  border: 1px solid #e9e9e9;
}
.cu-btn text {
  font-size: 16px;
  font-weight: 400;
}
.copyright {
  margin-top: 99px;
  margin-left: 36px;
  font-size: 12px;
  font-weight: 400;
  color: #898888;
}
.cu-form-group > uni-text {
  font-size: 14px;
}
.cu-btn uni-text {
  font-size: 14px;
  font-weight: 400;
  color: #fff;
}
.inputStyle {
  background: #42b0ed;
  color: #fff;
}
.noInputStyle {
  background: #f5f6f8;
  color: #898888;
}

