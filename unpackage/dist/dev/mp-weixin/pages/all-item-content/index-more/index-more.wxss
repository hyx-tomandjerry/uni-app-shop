.notice_title {
  font-size: 34rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}
.noticeItem {
  width: 100%;
  margin: 20rpx 0;
}
.noticeItem image {
  width: 100%;
}
.noticeItem .noticeInfo {
  font-size: 28rpx;
}
.title {
  font-size: 16px;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: #2a2a2a;
}
.cu-card > .cu-item {
  margin: 0;
}
.cu-card.article {
  border-bottom: 1px solid #eeeeed;
}
.cu-list.menu-avatar > .cu-item .content {
  left: 10px;
  width: calc(100% - 30px - 10px);
}

