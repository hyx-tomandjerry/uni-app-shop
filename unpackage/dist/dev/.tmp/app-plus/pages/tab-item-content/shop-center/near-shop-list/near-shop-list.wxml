<view bind:__l="__l"><view class="shop-content bg-white"><scroll-view scroll-y="true"><block wx:for="{{shopList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['choseShop',['$0'],[[['shopList','',index]]]]]]]}}" class="shop-list-item" bindtap="__e"><view class="shop-item-name flex justify-between"><view><text class="cuIcon-shop" style="margin-right:8px;"></text><text>{{item.name}}</text></view><block wx:if="{{item.catalogName}}"><text class="text-blue cuIcon-brand">{{item.catalogName}}</text></block></view><view class=".shop-item-address" style="width:100%;"><text class="cuIcon-location" style="margin-right:5px;"></text><text style="text-align:left;font-size:11px;">{{''+(item.provinceName||'')+(item.cityName||'')+(item.districtName||'')+item.address+''}}</text></view></view></block></scroll-view></view><block wx:if="{{type=='express'}}"><view style="position:fixed;bottom:0px;width:100%;"><view data-event-opts="{{[['tap',[['searchMore']]]]}}" class="cu-btn" style="width:30%;background:lightgray;" bindtap="__e">搜索更多</view><view data-event-opts="{{[['tap',[['createAddress']]]]}}" class="cu-btn bg-orange text-white" style="width:70%;" bindtap="__e"><text class="cuIcon-add" style="font-size:15px;margin-right:5px;"></text>创建新地址</view></view></block></view>