<view bind:__l="__l"><cu-custom isBack="{{true}}" vue-slots="{{['left','content','right']}}"><view slot="left"><text data-event-opts="{{[['tap',[['goBack']]]]}}" class="cuIcon-back" bindtap="__e"></text></view><view slot="content">反馈建议</view><view class="submit" slot="right"><text data-event-opts="{{[['tap',[['createSuggestion']]]]}}" bindtap="__e">提交</text></view></cu-custom><view class="suggestion-content"><view class="suggestion-content-title" style="padding-top:10px;"><text class="text-red">*</text>内容描述:</view><view><textarea style="max-height:142px;width:100%;padding-top:17px;padding-left:11px;background:rgba(247,247,247,1);border-radius:4px;margin-bottom:23px;" placeholder="告诉我们您遇到的问题，我们会第一时间处理" data-event-opts="{{[['input',[['__set_model',['$0','suggestion','$event',[]],['']]]]]}}" value="{{suggestion}}" bindinput="__e"></textarea></view><view class="suggestion-content-title" style="margin-bottom:19px;">截图(选填):</view><view><view class="cu-form-group"><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="padding-xs bg-img" style="{{'background-image:'+('url('+imgList[index]+')')+';'}}" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$0','file'],[[['imgList','',index]]]]]]]}}" bindtap="__e"><view class="cu-tag bg-red" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',[index,'file']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<4}}"><view data-event-opts="{{[['tap',[['chooseImageEvent']]]]}}" class="padding-xs solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view></view></view></view>